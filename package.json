{
	"name": "@alienfast/biome-config",
	"description": "AlienFast's Biome config.",
	"packageManager": "yarn@4.9.4",
	"version": "1.0.0",
	"license": "MIT",
	"repository": "https://github.com/alienfast/biome-config",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./src/base.jsonc"
		},
		"./base": {
			"import": "./src/base.jsonc"
		},
		"./react": {
			"import": "./src/react.jsonc"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build:ide": "echo 'tsc -b' && tsc -b",
		"clean": "tsx ./scripts/clean.ts",
		"clean:yarn": "tsx ./scripts/clean-yarn.ts",
		"reset": "tsx ./scripts/reset.ts",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"format": "biome format --write .",
		"test": "yarn check",
		"release": "yarn auto shipit"
	},
	"engines": {
		"node": ">=22"
	},
	"devDependencies": {
		"@alienfast/tsconfig": "^1.0.4",
		"@auto-it/all-contributors": "^11.3.0",
		"@auto-it/first-time-contributor": "^11.3.0",
		"@auto-it/released": "^11.3.0",
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^24.0.0",
		"auto": "^11.3.0",
		"execa": "^9.5.2",
		"rimraf": "^6.0.1",
		"tsx": "^4.19.2",
		"typescript": "^5.9.2"
	},
	"peerDependencies": {
		"@biomejs/biome": "*"
	},
	"author": "Kevin Ross <kevin.ross@alienfast.com>",
	"auto": {
		"canary": {
			"target": "comment"
		},
		"plugins": ["npm", "all-contributors", "first-time-contributor", "released"]
	}
}
